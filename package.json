{
    "name": "touch-bar-if-err",
    "displayName": "Touch Bar if err",
    "description": "if err != nil",
    "version": "0.0.1",
    "publisher": "D",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ifErr"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.ifErr",
                "title": "if err != nil { }"
            }
        ],
        "menus": {
            "touchBar": [
                {
                    "command": "extension.ifErr",
                    "group": "editing"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {}
}
